{
  "name": "discord-oidc",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/main.js",
    "dev": "concurrently -n \"TSC,NODE\" -c \"blue,green\" \"npm run watch:tsc\" \"npm run watch:node\"",
    "watch:tsc": "tsc -b -w --preserveWatchOutput",
    "watch:node": "DISCORD_OIDC_CONFIG=./config.yaml node --watch-path=./config.yaml --watch-path=./dist --watch-preserve-output dist/main.js",
    "test": "npm run build && node --import=nock --test dist/*.test.js dist/**/*.test.js",
    "issue-token": "node --import=tsx scripts/issue-token.ts"
  },
  "dependencies": {
    "@koa/router": "^15.2.0",
    "immutable": "^5.1.4",
    "koa": "^3.1.1",
    "koa-mount": "^4.2.0",
    "oidc-provider": "^9.6.0",
    "typebox": "^1.0.73",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/koa": "^3.0.1",
    "@types/koa-mount": "^4.0.5",
    "@types/node": "^25.0.9",
    "@types/oidc-provider": "^9.5.0",
    "concurrently": "^9.1.2",
    "discord-api-types": "^0.38.37",
    "fetch-cookie": "^3.2.0",
    "jwt-decode": "^4.0.0",
    "nock": "^14.0.10",
    "openid-client": "^6.8.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3"
  }
}
